                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT sa.OrderNumber, sa.ProductID, sa.OrderDate, 
CASE 
  WHEN sa.ShipDate IS NULL THEN 'No ship date'
  ELSE CONVERT(VARCHAR(10), sa.ShipDate, 111) -- convierte fecha a texto con formato 'yyyy-mm-dd'
END AS ShipDate,
